{
  "name": "its-valid",
  "version": "1.1.2",
  "description": "It's valid! - Typescript validators",
  "main": "index.js",
  "scripts": {
    "test": "jest --no-cache",
    "clean": "rm -rf {*.js,*.d.ts}",
    "compile": "npm run clean && tsc -p .",
    "prepublishOnly": "npm run test && npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wernerdegroot/its-valid.git"
  },
  "keywords": [
    "Typescript",
    "validators",
    "valid"
  ],
  "author": "Werner de Groot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wernerdegroot/its-valid/issues"
  },
  "homepage": "https://github.com/wernerdegroot/its-valid#readme",
  "devDependencies": {
    "@types/jest": "^22.1.2",
    "jest": "^22.3.0",
    "ts-jest": "^22.0.4",
    "typescript": "^2.7.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
